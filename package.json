{
  "name": "cry-bot",
  "version": "0.0.1",
  "description": "crypto trade bot - node console app",
  "keywords": [
    "crypto",
    "trade",
    "bot",
    "node",
    "console"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coeps-github/cry-bot.git"
  },
  "bugs": {
    "url": "https://github.com/coeps-github/cry-bot/issues"
  },
  "homepage": "https://github.com/coeps-github/cry-bot#readme",
  "author": "coeps",
  "license": "MIT",
  "engines": {
    "node": ">=14.13.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "lint": "eslint src/**/*.ts",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js --config config/crybot.json",
    "start:watch": "nodemon",
    "start:break": "nodemon --config nodemon-break.json",
    "update-dependencies": "npm-check-updates --upgrade && npm install"
  },
  "dependencies": {
    "asciichart": "1.5.25",
    "enquirer": "2.3.6",
    "minimist": "1.2.5",
    "node-binance-api": "0.12.2",
    "rxjs": "6.6.3",
    "trading-signals": "1.1.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/minimist": "1.2.0",
    "@types/node": "14.14.3",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "eslint": "7.12.0",
    "jest": "26.6.1",
    "nodemon": "2.0.6",
    "npm-check-updates": "9.2.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.2",
    "typescript": "4.0.3"
  }
}
